package pacoteArrays;

import java.util.*;

public class ClassArrays {

	public static void main(String[] args) {
		int [] numeros = new int[5];
		int total = 0;
		Scanner ent = new Scanner(System.in);
		
		System.out.println("Digite o 1º número. \n");
		numeros[0] = ent.nextInt();
		System.out.println("Digite o 2º número. \n");
		numeros[1] = ent.nextInt();
		System.out.println("Digite o 3º número. \n");
		numeros[2] = ent.nextInt();
		System.out.println("Digite o 4º número. \n");
		numeros[3] = ent.nextInt();
		System.out.println("Digite o 5º número. \n");
		numeros[4] = ent.nextInt();
		
		
		total = numeros[0] + numeros[1] + numeros[2] + numeros[3] + numeros[4];
		System.out.printf("O valor total é: %d", total);
		

	}

}
