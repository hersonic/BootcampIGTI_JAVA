package pacoteOperadoresLogicos;

public class ClasseOperadoresLogicos {

	public static void main(String[] args) {
		int var1 = 10, var2 = 20;
		boolean resultado;
		
		resultado = (var1 == var2);
		
		System.out.printf("As variaveis são iguais? \nResposta: %b", resultado);
		
		var1 = 20; 
		var2 = 20;
		
		resultado = (var1 != var2);
		
		System.out.printf("As variaveis são diferentes? \nResposta: %b", resultado);
		

	}

}
