package pacoteLACOS;

import java.util.Scanner;

public class ClasseLacosFor {

	public static void main(String[] args) {
		
		//for (inicialização: condição; incremento)
		
		int i = 0, j = 0;
		
		
		for(i = 0; i < 10; i++) {
			for (j = 0; i < 10; j++) {
				System.out.printf("%d * %d = %d.\n", i, j, i*j);
			}

		}
		
		

	}

}
