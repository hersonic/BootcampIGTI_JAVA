package pacoteOperadores;

public class ClasseOperadoresArimetricos {

	public static void main(String[] args) {
		int n1 = 0, n2 = 0, total = 0;
		n1 = 20;
		n2 = 30;
		total = n1 + n2;
		System.out.printf("O resultado de %d + %d = %d", n1, n2, total);
		
		n1 = 40;
		n2 = 50;
		total = n1 + n2;
		System.out.printf("O resultado de %d - %d = %d", n1, n2, total);
	}

}
